server
{
    listen 8080;
    host 127.0.0.1;
    server_name test;
    error_page 400 ./path.html;
    max_body_Size 5000;
    location /akatfi
    {
        root /nfs/homes/akatfi/Desktop/coding_web/akatfi;
        autoindex ON;
        allowed_methods GET;
    } 
    location /afadlane
    {
        root /nfs/homes/akatfi/Desktop/coding_web/afadlane;
        autoindex ON;
        allowed_methods GET;
    }
    location /
    {
        root /nfs/homes/akatfi/Desktop/coding_web/www;
        uploadfile ON;
        upload_location source;
        autoindex ON;
        allowed_methods GET POST;
    }      
}
