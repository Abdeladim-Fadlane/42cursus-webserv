server
{
    listen 8081;
    host 127.0.0.1;
    cgi_timeout 10;
    error_page 404 index.html;
    max_body_Size 500;
    location /dir
    {
        root /nfs/homes/afadlane/webserv;
        upload_location /nfs/homes/afadlane/;
        cgi     /usr/bin/php-cgi8.1 .php;
    }  
}
