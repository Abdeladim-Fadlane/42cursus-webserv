server
{
    listen 8080;
    host 127.0.0.1;
    server_name test;
    error_page 400 ./path.html;
    max_body_Size 5000;
    location /akatfi
    {
        root /nfs/homes/akatfi/Desktop/main_bransh/akatfi;
        autoindex ON;
        cgi_allowed ON;
        index index.php;
        cgi     /usr/bin/php-cgi8.1 .php;
        allowed_methods GET;
    } 
    location /afadlane
    {
        root /nfs/homes/akatfi/Desktop/main_bransh/afadlane;
        autoindex ON;
        cgi_allowed ON;
        index phpinfo.php index.html;
        allowed_methods GET;
        cgi     /usr/bin/python3 .py;
        cgi     /usr/bin/php-cgi8.1 .php;
        cgi     /bin/bash .sh ; 
    }
    location /
    {
        root /nfs/homes/akatfi/Desktop/coding_web/www;
        uploadfile ON;
        upload_location source;
        autoindex ON;
        allowed_methods GET POST;
    }      
}
